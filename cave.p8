function box_hit(
	x1,y1,
	w1,h1,
	x2,y2,
	w2,h2)
	
	local hit=false
	
	local xs=w1/2 + w2/2
	local ys=h1/2 + h2/2
	local xd=abs((x1 + (w1/2)) - (x2 + (w2/2)))
	local yd=abs((y1 + (h1/2)) - (y2 + (h2/2)))
	
	if (xd<xs and yd<ys) then
		hit=true
	end
	
	return hit
end

function update_menu()
	if (btnp(❎)) then
		scene="game"
		music(-1)
	end
end

function draw_menu()
	cls()
	map(16,0,0,0,16,16)
	--print("menu", 44, 44, 7)
	print("press ❎ to start", 32, 80, 0)
end
